# Variables d'Environnement Railway

# Obligatoires
SECRET_KEY=<générez-une-clé-secrète-forte>
DEBUG=False

# Optionnelles (valeurs par défaut déjà configurées)
ALLOWED_HOSTS=.railway.app,.up.railway.app
CSRF_TRUSTED_ORIGINS=https://*.railway.app,https://*.up.railway.app

# DATABASE_URL est automatiquement fourni par Railway quand vous ajoutez PostgreSQL
# Pas besoin de le configurer manuellement !
